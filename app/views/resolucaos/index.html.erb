<p id="notice"><%= notice %></p>

<h1>Listing Resolucaos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Status final</th>
      <th>Obs resolucao</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resolucaos.each do |resolucao| %>
      <tr>
        <td><%= resolucao.descricao %></td>
        <td><%= resolucao.status_final %></td>
        <td><%= resolucao.obs_resolucao %></td>
        <td><%= resolucao.created_by %></td>
        <td><%= resolucao.updated_by %></td>
        <td><%= link_to 'Show', resolucao %></td>
        <td><%= link_to 'Edit', edit_resolucao_path(resolucao) %></td>
        <td><%= link_to 'Destroy', resolucao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resolucao', new_resolucao_path %>
